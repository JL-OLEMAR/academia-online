{
  "name": "academia-online",
  "version": "1.0.0",
  "description": "API GraphQL Academia Online",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,json,graphql",
    "publish": "npm run build && vercel --prod"
  },
  "keywords": [
    "GraphQL",
    "Apollo Server"
  ],
  "author": "Jose Olemar <joseluis19963@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.24.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-import-node": "^0.0.4",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-tools": "^7.0.5",
    "http": "^0.0.1-security",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "standard": "^16.0.3"
  }
}
